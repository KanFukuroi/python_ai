        "numbers = [10, 13, 14, 15, 20, 27, 30]\n",
        "even_sum = 0\n",
        "odd_sum = 0\n",
        "\n",
        "for i in numbers:\n",
        "    if i % 2 == 0:\n",
        "        even_sum += i\n",
        "    else:\n",
        "        odd_sum += i\n",
        "print(\"Even sum:\", even_sum)\n",
        "print(\"Odd sum:\", odd_sum)\n"
      ]
